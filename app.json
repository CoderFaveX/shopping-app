{
  "expo": {
    "name": "project-shopping",
    "slug": "shopping-app",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fave2009.shoppingapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fave2009.shoppingapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "Poppins": {
            "100": "assets/fonts/Poppins-Thin.ttf",
            "200": "assets/fonts/Poppins-ExtraLight.ttf",
            "300": "assets/fonts/Poppins-Light.ttf",
            "400": "assets/fonts/Poppins-Regular.ttf",
            "500": "assets/fonts/Poppins-Medium.ttf",
            "600": "assets/fonts/Poppins-SemiBold.ttf",
            "700": "assets/fonts/Poppins-Bold.ttf",
            "800": "assets/fonts/Poppins-ExtraBold.ttf",
            "900": "assets/fonts/Poppins-Black.ttf"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "40563956-d74d-4d2a-9e77-f2f7c13438e9"
      }
    },
    "owner": "fave2009"
  }
}
